{
  "name": "@bui/ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "tsc && node ./scripts/generate-react-jsx-types.js",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bui/icons": "workspace:*",
    "bitcoin-qr": "^1.4.1",
    "lit": "^3.3.0",
    "@lit/react": "^1.0.6"
  },
  "type": "module",
  "module": "dist/index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/react-jsx.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "types",
    "react.d.ts",
    "*.ts",
    "*.js",
    "*.d.ts",
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./react": {
      "types": "./react.d.ts",
      "default": "./react.js"
    },
    "./button.js": { "types": "./types/react-jsx.d.ts", "default": "./dist/button.js" },
    "./toggle.js": { "types": "./types/react-jsx.d.ts", "default": "./toggle.js" },
    "./message.js": { "types": "./types/react-jsx.d.ts", "default": "./dist/message.js" },
    "./numpad-button.js": { "types": "./types/react-jsx.d.ts", "default": "./numpad-button.js" },
    "./button-cluster.js": { "types": "./types/react-jsx.d.ts", "default": "./button-cluster.js" },
    "./input.js": { "types": "./types/react-jsx.d.ts", "default": "./input.js" },
    "./money-value.js": { "types": "./types/react-jsx.d.ts", "default": "./money-value.js" },
    "./bitcoin-value.js": { "types": "./types/react-jsx.d.ts", "default": "./bitcoin-value.js" },
    "./option-dot.js": { "types": "./types/react-jsx.d.ts", "default": "./option-dot.js" },
    "./bitcoin-qr-display.js": { "types": "./types/react-jsx.d.ts", "default": "./bitcoin-qr-display.js" },
    "./tokens.css": "./tokens.css"
  },
  "style": "../tokens/lib/tailwindcss/tailwind-theme.css",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/web-components": "^9.0.16",
    "@storybook/web-components-vite": "^9.0.16",
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "playwright": "^1.54.1",
    "storybook": "^9.0.16",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
