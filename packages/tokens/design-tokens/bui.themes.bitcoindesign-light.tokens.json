{
  "button": {
    "filled": {
      "bg": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "outline": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.active}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.active.alternate}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        }
      },
      "disabled": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.disabled}"
        },
        "outline": {
          "$type": "color",
          "$value": "{button.filled.disabled.bg}"
        },
        "text": {
          "$type": "color",
          "$value": "{button.filled.text}"
        }
      }
    },
    "outline": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.orange.50}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        },
        "text": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.orange.100}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.active.alternate}"
        },
        "text": {
          "$type": "color",
          "$value": "{system.interactive}"
        }
      },
      "text": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "disabled": {
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.disabled}"
        },
        "text": {
          "$type": "color",
          "$value": "{system.interactive.disabled}"
        }
      }
    },
    "free": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "text": {
        "$type": "color",
        "$value": "{black}"
      },
      "disabled": {
        "text": {
          "$type": "color",
          "$value": "{tailwind.zinc.500}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.300}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.400}"
        }
      }
    },
    "rounding": {
      "$type": "dimension",
      "$value": "{size.1}"
    }
  },
  "background": {
    "$type": "color",
    "$value": "{white}"
  },
  "text": {
    "primary": {
      "$type": "color",
      "$value": "{black}"
    },
    "secondary": {
      "$type": "color",
      "$value": "{tailwind.zinc.500}"
    }
  },
  "Color": {
    "$type": "color",
    "$value": "#ffffff"
  },
  "system": {
    "interactive": {
      "$type": "color",
      "$value": "{tailwind.orange.400}",
      "active": {
        "$type": "color",
        "$value": "{tailwind.orange.600}",
        "alternate": {
          "$type": "color",
          "$value": "{tailwind.orange.300}"
        }
      },
      "disabled": {
        "$type": "color",
        "$value": "{tailwind.orange.300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{tailwind.orange.500}"
      }
    },
    "mood": {
      "danger": {
        "$type": "color",
        "$value": "#cc0000",
        "active": {
          "$type": "color",
          "$value": "#cc000080"
        },
        "text": {
          "$type": "color",
          "$value": "#910000"
        }
      },
      "caution": {
        "$type": "color",
        "$value": "#ffbd00",
        "active": {
          "$type": "color",
          "$value": "#ffbd0080"
        },
        "text": {
          "$type": "color",
          "$value": "#997100"
        }
      },
      "success": {
        "$type": "color",
        "$value": "#12b65b",
        "active": {
          "$type": "color",
          "$value": "#12b65b80"
        },
        "text": {
          "$type": "color",
          "$value": "#0c783b"
        }
      },
      "neutral": {
        "$type": "color",
        "$value": "{tailwind.zinc.400}",
        "active": {
          "$type": "color",
          "$value": "#f5490080"
        },
        "text": {
          "$type": "color",
          "$value": "{text.secondary}"
        }
      }
    },
    "divider": {
      "$type": "color",
      "$value": "{tailwind.zinc.300}"
    }
  },
  "component": {
    "input": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "placeholder": {
        "$type": "color",
        "$value": "{tailwind.zinc.400}"
      },
      "rounding": {
        "large": {
          "$type": "dimension",
          "$value": "{size.1}"
        },
        "small": {
          "$type": "dimension",
          "$value": "{size.1}"
        }
      }
    },
    "numpad": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.zinc.100}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.orange.200}"
        }
      }
    },
    "toggle": {
      "bg": {
        "$type": "color",
        "$value": "{tailwind.zinc.500}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive}"
        }
      },
      "switch": {
        "$type": "color",
        "$value": "{white}"
      }
    }
  }
}