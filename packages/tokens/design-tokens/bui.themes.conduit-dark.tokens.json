{
  "button": {
    "filled": {
      "bg": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "outline": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.active.alternate}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.blue.500}"
        },
        "outline": {
          "$type": "color",
          "$value": "{tailwind.blue.500}"
        }
      },
      "disabled": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.disabled}"
        },
        "outline": {
          "$type": "color",
          "$value": "{button.filled.disabled.bg}"
        },
        "text": {
          "$type": "color",
          "$value": "{button.filled.text}"
        }
      }
    },
    "outline": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{white}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.900}"
        },
        "outline": {
          "$type": "color",
          "$value": "{white}"
        },
        "text": {
          "$type": "color",
          "$value": "{white}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.blue.950}"
        },
        "outline": {
          "$type": "color",
          "$value": "{tailwind.blue.200}"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.blue.200}"
        }
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "disabled": {
        "outline": {
          "$type": "color",
          "$value": "{tailwind.sky.800}"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.sky.800}"
        }
      }
    },
    "free": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "disabled": {
        "text": {
          "$type": "color",
          "$value": "{tailwind.zinc.500}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.900}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.800}"
        }
      }
    },
    "rounding": {
      "$type": "dimension",
      "$value": "{size.6}"
    }
  },
  "background": {
    "$type": "color",
    "$value": "{tailwind.slate.900}"
  },
  "text": {
    "primary": {
      "$type": "color",
      "$value": "{white}"
    },
    "secondary": {
      "$type": "color",
      "$value": "{tailwind.slate.300}"
    }
  },
  "Color": {
    "$type": "color",
    "$value": "#ffffff"
  },
  "system": {
    "interactive": {
      "$type": "color",
      "$value": "{tailwind.blue.600}",
      "active": {
        "$type": "color",
        "$value": "{tailwind.blue.700}",
        "alternate": {
          "$type": "color",
          "$value": "{tailwind.blue.950}"
        }
      },
      "disabled": {
        "$type": "color",
        "$value": "{tailwind.sky.950}"
      },
      "hover": {
        "$type": "color",
        "$value": "{tailwind.blue.500}"
      }
    },
    "mood": {
      "danger": {
        "$type": "color",
        "$value": "#fc3b3b",
        "active": {
          "$type": "color",
          "$value": "#fc3b3b80"
        },
        "text": {
          "$type": "color",
          "$value": "#ffbbbb"
        }
      },
      "caution": {
        "$type": "color",
        "$value": "#ffca34",
        "active": {
          "$type": "color",
          "$value": "#ffca3480"
        },
        "text": {
          "$type": "color",
          "$value": "#ffebb1"
        }
      },
      "success": {
        "$type": "color",
        "$value": "#12b65b",
        "active": {
          "$type": "color",
          "$value": "#12b65b80"
        },
        "text": {
          "$type": "color",
          "$value": "#82f2b3"
        }
      },
      "neutral": {
        "$type": "color",
        "$value": "{tailwind.slate.500}",
        "active": {
          "$type": "color",
          "$value": "#1447e680"
        },
        "text": {
          "$type": "color",
          "$value": "{text.secondary}"
        }
      }
    },
    "divider": {
      "$type": "color",
      "$value": "{tailwind.slate.800}"
    }
  },
  "component": {
    "input": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "placeholder": {
        "$type": "color",
        "$value": "{tailwind.slate.700}"
      },
      "rounding": {
        "large": {
          "$type": "dimension",
          "$value": "{size.6}"
        },
        "small": {
          "$type": "dimension",
          "$value": "{size.4}"
        }
      }
    },
    "numpad": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.slate.800}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.blue.950}"
        }
      }
    },
    "toggle": {
      "bg": {
        "$type": "color",
        "$value": "{tailwind.slate.700}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive}"
        }
      },
      "switch": {
        "$type": "color",
        "$value": "{white}"
      }
    }
  }
}