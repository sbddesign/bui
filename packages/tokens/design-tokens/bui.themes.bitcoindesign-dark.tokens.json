{
  "button": {
    "filled": {
      "bg": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "outline": {
        "$type": "color",
        "$value": "{system.interactive}"
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.active}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.active.alternate}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        },
        "outline": {
          "$type": "color",
          "$value": "{system.interactive.hover}"
        }
      },
      "disabled": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive.disabled}"
        },
        "outline": {
          "$type": "color",
          "$value": "{button.filled.disabled.bg}"
        },
        "text": {
          "$type": "color",
          "$value": "#ffffff80"
        }
      }
    },
    "outline": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{white}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.900}"
        },
        "outline": {
          "$type": "color",
          "$value": "{tailwind.orange.100}"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.orange.100}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.900}"
        },
        "outline": {
          "$type": "color",
          "$value": "{tailwind.orange.100}"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.orange.100}"
        }
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "disabled": {
        "outline": {
          "$type": "color",
          "$value": "{tailwind.gray.500}"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.gray.500}"
        }
      }
    },
    "free": {
      "bg": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "outline": {
        "$type": "color",
        "$value": "{transparent}"
      },
      "text": {
        "$type": "color",
        "$value": "{white}"
      },
      "disabled": {
        "text": {
          "$type": "color",
          "$value": "{tailwind.zinc.500}"
        }
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.900}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.gray.800}"
        }
      }
    },
    "rounding": {
      "$type": "dimension",
      "$value": "{size.1}"
    }
  },
  "background": {
    "$type": "color",
    "$value": "{black}"
  },
  "text": {
    "primary": {
      "$type": "color",
      "$value": "{white}"
    },
    "secondary": {
      "$type": "color",
      "$value": "{tailwind.zinc.300}"
    }
  },
  "Color": {
    "$type": "color",
    "$value": "#ffffff"
  },
  "system": {
    "interactive": {
      "$type": "color",
      "$value": "{tailwind.orange.400}",
      "active": {
        "$type": "color",
        "$value": "{tailwind.orange.500}",
        "alternate": {
          "$type": "color",
          "$value": "{tailwind.red.900}"
        }
      },
      "disabled": {
        "$type": "color",
        "$value": "{tailwind.orange.900}"
      },
      "hover": {
        "$type": "color",
        "$value": "{tailwind.orange.300}"
      }
    },
    "mood": {
      "danger": {
        "$type": "color",
        "$value": "{tailwind.red.500}",
        "active": {
          "$type": "color",
          "$value": "#fb2c3680"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.red.300}"
        }
      },
      "caution": {
        "$type": "color",
        "$value": "{tailwind.amber.400}",
        "active": {
          "$type": "color",
          "$value": "#ffb90080"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.amber.100}"
        }
      },
      "success": {
        "$type": "color",
        "$value": "{tailwind.green.500}",
        "active": {
          "$type": "color",
          "$value": "#00c95080"
        },
        "text": {
          "$type": "color",
          "$value": "{tailwind.green.200}"
        }
      },
      "neutral": {
        "$type": "color",
        "$value": "{tailwind.zinc.500}",
        "active": {
          "$type": "color",
          "$value": "#ff690080"
        },
        "text": {
          "$type": "color",
          "$value": "{text.secondary}"
        }
      }
    },
    "divider": {
      "$type": "color",
      "$value": "{tailwind.zinc.800}"
    }
  },
  "component": {
    "input": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "placeholder": {
        "$type": "color",
        "$value": "{tailwind.zinc.700}"
      },
      "rounding": {
        "large": {
          "$type": "dimension",
          "$value": "{size.1}"
        },
        "small": {
          "$type": "dimension",
          "$value": "{size.1}"
        }
      }
    },
    "numpad": {
      "bg": {
        "$type": "color",
        "$value": "{background}"
      },
      "hover": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.zinc.800}"
        }
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{tailwind.orange.800}"
        }
      }
    },
    "toggle": {
      "bg": {
        "$type": "color",
        "$value": "{tailwind.zinc.700}"
      },
      "active": {
        "bg": {
          "$type": "color",
          "$value": "{system.interactive}"
        }
      },
      "switch": {
        "$type": "color",
        "$value": "{white}"
      }
    }
  }
}